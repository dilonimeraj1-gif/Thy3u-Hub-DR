		v:Destroy()
	end
end
    end
})

local Button = MainTab:CreateButton({
    Name = "Kill Everybody With Pummel Stick",
    Description = "Only in pond challenge.", -- Creates A Description For Users to know what the button does (looks bad if you use it all the time),
    Callback = function()
        local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local LocalPlayer = Players.LocalPlayer
local Sword = LocalPlayer.Backpack:FindFirstChild("Sword")

local function getHead(character)
    return character:FindFirstChild("Torso")
end

local function SitBehindHead(targetPlayer)
    if not targetPlayer.Character or not getHead(targetPlayer.Character) then
        return
    end

    local targetCharacter = targetPlayer.Character
    local targetHead = getHead(targetCharacter)
    local character = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
    local humanoid = character:FindFirstChildOfClass("Humanoid")
    local root = character:FindFirstChild("HumanoidRootPart")

    if not humanoid or not root then
        return
    end

    humanoid.Sit = true

    local headSit
    headSit = RunService.Heartbeat:Connect(function()
        if Players:FindFirstChild(targetPlayer.Name) and targetCharacter ~= nil and targetHead and root and humanoid.Sit == true then
            root.CFrame = targetHead.CFrame * CFrame.new(-1.75, -1, 4.25)
            if Sword and Sword:FindFirstChild("RemoteClick") then
                local clickEvent = Sword.RemoteClick
                if clickEvent then
                    clickEvent:FireServer()
                end
            end
        else
            headSit:Disconnect()
        end
    end)

    local targetHumanoid = targetCharacter:FindFirstChild("Humanoid")
    while targetHumanoid and targetHumanoid.Health > 0 do
        wait(0.1)
    end

    if headSit then
        headSit:Disconnect()
    end
end

local function SitOnEveryone()
    for _, player in ipairs(Players:GetPlayers()) do
        if player ~= LocalPlayer then
            SitBehindHead(player)
        end
    end
end

SitOnEveryone()
    end
})

