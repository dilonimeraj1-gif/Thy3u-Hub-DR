local Button = MainTab:CreateButton({
    Name = "Win Pot And Keys.",
    Description = "Wins the pot and keys challenge", -- Creates A Description For Users to know what the button does (looks bad if you use it all the time),
    Callback = function()
       local key = workspace:FindFirstChild("Key", true)
if key and key:FindFirstChild("TouchInterest") then
    firetouchinterest(game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart"), key, 0)
    firetouchinterest(game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart"), key, 1)
end

wait(0.1)

for _, model in pairs(workspace:GetDescendants()) do
    if model:IsA("Model") then
        for _, obj in pairs(model:GetDescendants()) do
            if obj:IsA("ProximityPrompt") then
                fireproximityprompt(obj)
            end
        end
    end
end
    end
})

