    Name = "Remove Shark",
    Description = "Removes it", -- Creates A Description For Users to know what the button does (looks bad if you use it all the time),
    Callback = function()
       for i, v in pairs(workspace.Assets:GetDescendants()) do
            if v.Name == "Shark" then
                v:Destroy()
            end
        end
    end
})

